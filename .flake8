[flake8]
max-line-length=99
max-doc-length=99
max-complexity=10
#extend=.svn,CVS,.bzr,.hg,.git,__pycache__,.tox,.eggs,*.egg
extend-exclude=venv,dist,conf,scripts,log_app,env
extend-ignore = E999
exclude =
    migrations
    .git
    __pycache__
    pytest_cache
    .pytest_cache
    ./pytest_cache
    pytest_cache/
    .pytest_cache/
    env
    .env
    ./env
    env/
    .env/
    venv
    .venv
    ./venv
    venv/
    .venv/
     dev
    .dev
    ./dev
    dev/
    .dev/
    logs
    .logs
    ./logs
    logs/
    .logs/